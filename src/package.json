{
  "name": "skeleton-sprint-levelbuilder",
  "version": "1.0.0",
  "description": "levelbuilder for skeleton sprint",
  "main": "main.js",
  "scripts": {
    "test": "electron .",
    "build": "electron-builder build -w",
    "build:neon": "electron-build-env neon build sks-neon --release",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adumbidiot/skeleton-sprint-levelbuilder.git"
  },
  "keywords": [
    "skeleton",
    "sprint",
    "level",
    "builder"
  ],
  "author": "adumbidiot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adumbidiot/skeleton-sprint-levelbuilder/issues"
  },
  "homepage": "https://github.com/adumbidiot/skeleton-sprint-levelbuilder#readme",
  "build": {
    "win": {
      "target": "portable",
      "icon": "images/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^3.1.8",
    "electron-build-env": "^0.2.0",
    "electron-builder": "^20.0.0",
    "electron-rebuild": "1.6.0",
    "neon-cli": "^0.3.1"
  },
  "dependencies": {
    "@hapi/cryptiles": ">=4.1.2",
    "ajv": "^6.10.2",
    "greenworks": "git+https://github.com/greenheartgames/greenworks.git#ac155afecc2c4edbc00bd1c81514ae1ad5e04496",
    "hoek": "5.0.3",
    "nan": "^2.14.0",
    "sks-neon": "file:native/sks-neon"
  }
}
